public class apples {
	public static void main(String[] args) {
		int amount;
		double principal = 10000;
		double rate = .01;
		
		for(int day=1;day<=20;day++) {
			amount= (int) (principal*Math.pow(1 + rate, day));
			System.out.println("Bucky will probably have " + amount + " subscribres on day " + day);
		}
	}
}
