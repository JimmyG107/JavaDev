public class apples {
	public static void main(String[] args) {
		tuna tunaClass = new tuna();
		System.out.println(tunaClass.toMilitary());
		System.out.println(tunaClass.toStandard());
		tunaClass.setTime(13, 27, 6);
		System.out.println(tunaClass.toMilitary());
		System.out.println(tunaClass.toStandard());
	}
}