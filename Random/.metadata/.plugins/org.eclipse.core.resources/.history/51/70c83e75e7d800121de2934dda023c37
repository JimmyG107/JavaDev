import java.util.Scanner;
public class apples {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		tuna tunaClass = new tuna("Jordan");
		tuna tunaClass2 = new tuna("Morgan");
		System.out.print("Enter the name of your first girlfriend here:  ");
		String temp = input.nextLine();
		tunaClass.setName(temp);
		tunaClass.saying();
		tunaClass2.saying();
	}
}
