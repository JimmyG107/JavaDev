public class apples {
	public static void main(String[] args) {
		tuna tunaClass = new tuna();
		tuna tunaClass2 = new tuna(5);
		tuna tunaClass3 = new tuna(5,13);
		tuna tunaClass4 = new tuna(5,13,43);
		
		System.out.printf("%s\n", tunaClass.toMilitary());
		System.out.printf("%s\n", tunaClass2.toMilitary());
		System.out.printf("%s\n", tunaClass3.toMilitary());
		System.out.printf("%s\n", tunaClass4.toMilitary());

	}
}